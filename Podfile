# Uncomment the next line to define a global platform for your project
platform :ios, '8.3'

use_frameworks!

def base_framework
    pod ‘UMengUShare/UI‘
    pod ‘UMengUShare/Social/Sina'
    pod ‘UMengUShare/Social/WeChat'
    pod ‘UMengUShare/Social/QQ'
    pod 'UMengAnalytics', '~> 4.1.2' #UM 统计分析
    pod 'JPush', '~> 3.0.6' #APNs框架
    pod 'OpenUDID', '~> 1.0.0' #Open UDID
    pod 'Qiniu'  #七牛
    pod 'CocoaLumberjack/Swift' #日志管理
    pod 'RxSwift', '~> 4.1.0'
    pod 'RxCocoa', '~> 4.1.0'
    pod 'RxDataSources', '~> 3.0'
    pod 'Moya/RxSwift', '~> 10.0.0'
    pod 'Alamofire', '~> 4.5.0'
    pod 'ObjectMapper', '~> 3.1.0'
    pod 'SnapKit', '~>4.0'
    pod 'RealmSwift'
    pod 'Kingfisher', '~> 4.5.0'
    pod 'ObjectMapper+Realm'
    pod 'R.swift', '~> 4.0.0'
    pod 'URLNavigator', '~> 2.0.4'
    pod 'SwiftProtobuf', '~> 1.0.2'
    pod 'NSObject+Rx', '~> 4.2.0'
    pod 'Spring', '~> 1.0.5' #动画
    pod 'MGSwipeTableCell' #左滑
    pod 'PKHUD', '~> 5.0' #toast
    pod 'FreeStreamer', '~> 3.8.0' #高端音乐播放器
end

target 'OneWayDemo' do

  base_framework

end

